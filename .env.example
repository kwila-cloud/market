# Frontend environment variables
# Copy this file to .env and update values as needed

# Supabase connection for the Astro frontend
# These use the PUBLIC_ prefix to be available in the browser

# For local development:
# 1. Run `npm run start:backend`
# 2. Copy the "Publishable key" from the output
PUBLIC_SUPABASE_URL=http://localhost:54321
PUBLIC_SUPABASE_ANON_KEY=your-publishable-key-here
