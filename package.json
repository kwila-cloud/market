{
  "name": "market",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "astro check",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:e2e": "npx playwright test",
    "test:watch": "vitest",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:restart": "docker compose restart",
    "docker:logs": "docker compose logs -f",
    "docker:db:reset": "docker compose down -v && docker compose up -d && npm run docker:db:wait && npm run docker:db:migrate && npm run docker:db:seed",
    "docker:db:wait": "echo 'Waiting for database to be ready...' && sleep 10",
    "docker:db:migrate": "docker compose exec -T db bash /docker-entrypoint-initdb.d/run-migrations.sh",
    "docker:db:seed": "docker compose exec -T db psql -U postgres -d postgres -f /docker-entrypoint-initdb.d/seed.sql",
    "docker:studio": "echo 'Supabase Studio: http://localhost:3000'"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/cloudflare": "^12.6.10",
    "@astrojs/react": "^4.4.2",
    "@tailwindcss/vite": "^4.1.17",
    "astro": "^5.15.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vite-pwa/astro": "^1.1.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.9",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "tailwindcss": "^4.1.17",
    "vitest": "^4.0.9",
    "wrangler": "^4.46.0"
  }
}
